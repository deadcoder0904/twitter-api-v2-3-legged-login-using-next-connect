{
  "private": true,
  "scripts": {
    "predev": "npm run generate",
    "dev": "next",
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "generate": "npm run generate:prisma && npm run generate:nexus && npm run generate:graphql-codegen",
    "generate:prisma": "prisma generate",
    "generate:nexus": "cross-env GENERATE=true cross-env DOTENV_CONFIG_PATH=.env.example ts-node -r tsconfig-paths/register --require dotenv/config --transpile-only -P nexus.tsconfig.json server/graphql/schema.ts",
    "generate:graphql-codegen": "graphql-codegen"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^0.5.2-next.19",
    "@prisma/client": "^3.3.0",
    "apollo-server-micro": "^3.4.0",
    "graphql": "^15.7.0",
    "graphql-middleware": "^6.0.9",
    "graphql-shield": "^7.5.0",
    "next": "^12.0.1",
    "next-auth": "^4.0.0-beta.4",
    "next-connect": "^0.11.0",
    "nexus": "^1.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "twitter-api-v2": "^1.6.4",
    "urql": "^2.0.5"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/near-operation-file-preset": "^2.1.6",
    "@graphql-codegen/plugin-helpers": "^2.2.0",
    "@graphql-codegen/typed-document-node": "^2.1.6",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-codegen/typescript-operations": "^2.1.8",
    "@graphql-codegen/typescript-urql": "^3.2.2",
    "@types/node": "^16.11.6",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "apollo-server-core": "^3.4.0",
    "cross-env": "^7.0.3",
    "prisma": "^3.3.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4"
  },
  "prisma": {
    "schema": "server/db/schema.prisma"
  }
}
